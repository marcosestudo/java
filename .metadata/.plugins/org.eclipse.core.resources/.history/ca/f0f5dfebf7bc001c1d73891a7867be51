public class Program {

	public static void main(String[] args) {
		// membros estáticos podem ser chamados de membros de classe
		// Não precisam de instanciados para serem usador
		// Como exemplo de mebro estático temos a função Math.sqrt()

		
		Locale.setDefault(Locale.US);
		Scanner sc = new Scanner(System.in);
		
		
		
		
		sc.close();
	}

}
